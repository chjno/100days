{
  "name": "Page Burn",
  "manifest_version": 2,
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content.js"]
  }],
  "permissions": [
    "<all_urls>",
    "tabs",
    "webNavigation"
  ]
}
