{
  "name": "Document Body",
  "manifest_version": 3,
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["system.display", "nativeMessaging"],
  "web_accessible_resources": [
    {
      "resources": ["limbs/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
